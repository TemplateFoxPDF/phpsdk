{
    "name": "templatefox/sdk",
    "version": "1.0.0",
    "description": "Official TemplateFox PHP SDK - Generate PDFs from HTML templates",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/TemplateFoxPDF/phpsdk",
    "license": "MIT",
    "authors": [
        {
            "name": "TemplateFox",
            "homepage": "https://pdftemplateapi.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "TemplateFox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TemplateFox\\Test\\": "test/"
        }
    }
}
